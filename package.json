{
  "name": "safe-decode-uri-component",
  "version": "1.2.2-native",
  "description": "Safely decode URI components, leaving invalid sequences as they are",
  "main": "src/index.js",
  "scripts": {
    "test": "node tests/index",
    "benchmark": "node benchmark",
    "install": "prebuild-install --runtime napi || node-gyp rebuild",
    "build": "prebuild --compile",
    "rebuild": "prebuild --compile",
    "prebuild": "prebuild --all --runtime napi --strip --verbose",
    "prebuild:mac-x64": "npm run prebuild -- --arch x64 --platform darwin",
    "prebuild:mac-apple": "npm run prebuild -- --arch arm64 --platform darwin",
    "prebuild:linux": "npm run prebuild -- --arch x64 --platform linux",
    "prebuild:windows": "npm run prebuild -- --arch x64 --platform windows",
    "prepublishOnly": "run-s prebuild:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jridgewell/safe-decode-uri-component.git"
  },
  "keywords": [
    "decode",
    "uri",
    "component"
  ],
  "author": "Justin Ridgewell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jridgewell/safe-decode-uri-component/issues"
  },
  "homepage": "https://github.com/jridgewell/safe-decode-uri-component#readme",
  "engines": {
    "node": ">=8.6.0"
  },
  "binary": {
    "napi_versions": [
      1,
      2,
      3
    ]
  },
  "gypfile": true,
  "devDependencies": {
    "benchmark": "2.1.4",
    "microtime": "3.1.1",
    "prebuild": "12.1.0"
  },
  "dependencies": {
    "bindings": "1.5.0",
    "node-gyp": "9.4.0",
    "prebuild-install": "7.1.1"
  }
}
